@{
    ViewData["Title"] = "Clientes";
}

<div class="text-center">

    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-6 login-section-wrapper">
                <div class="brand-wrapper">
                    <img src="https://www.piktotal.com.br/wp-content/themes/piktotal/assets/img/titulo-buffet.png" alt="logo" class="logo">
                </div>
                <div class="login-wrapper my-auto">
                    <h1 class="login-title">Cadastro Local</h1>

                    <form action="" method="post" class="text-left">

                        <div class="form-group">
                            <label for="eventDesc">Decrição</label>
                            <input type="text" name="eventDesc" id="eventDesc" class="form-control" placeholder="Digite a descrição..">
                        </div>
                        <div class="form-group">
                            <label for="eventEnd">Endereço</label>
                            <input type="text" name="eventEnd" id="eventEnd" class="form-control" placeholder="Digite o endereço..">
                        </div>

                        <input name="Cadastrar" id="Cadastrar" class="btn-block btn btn-secondary" type="submit" value="Cadastrar">
                        <p class="login-wrapper-footer-text">
                            <a asp-area="" asp-controller="Interno" asp-action="Locais" class="text-reset">Voltar</a>
                        </p>
                    </form>

                </div>
            </div>

        </div>
    </div>

</div>

@section Scripts{
    <script>
  
  </script>
}

;